<Window x:Class="Sudoku.WindowNewName"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku"
        mc:Ignorable="d"
        Title="Enter your name" Height="80" Width="300" Icon="smile.ico">
    <Grid>
        <TextBox x:Name="TextBlockName" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="190" KeyUp="TextBlockName_KeyUp"/>
        <Button x:Name="ButtonEnter" Content="Enter" HorizontalAlignment="Left" Margin="205,13,0,0" VerticalAlignment="Top" Width="75" Click="ButtonEnter_Click"/>

    </Grid>
</Window>
